{
  "name": "@artemis/ui",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./theme": "./src/theme/index.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --ignore-path ../../.prettierignore --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --ignore-path ../../.prettierignore --check \"**/*.{ts,tsx,md}\"",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "peerDependencies": {
    "@expo/vector-icons": "*",
    "expo-linear-gradient": "*",
    "react": "*",
    "react-native": "*",
    "react-native-safe-area-context": "*"
  },
  "devDependencies": {
    "@artemis/eslint-config": "workspace:*",
    "@artemis/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "~19.1.10",
    "typescript": "5.9.2"
  }
}
